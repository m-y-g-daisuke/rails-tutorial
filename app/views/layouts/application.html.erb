<!DOCTYPE html>
<html>
  <%= render 'layouts/rails_default'%>
  <body>
  <% flash.each do |message_type, message|%>
    <%= tag.div message, class:"alert alert-#{message_type}" %>
  <% end %>
    <%= render 'layouts/header'%>
    <div class="container">
      <%= yield %>
      <%= render 'layouts/footer'%>
      <%= debug(params) if Rails.env.development?%>
    </div>
  </body>
</html>
